                    <Ellipse.Fill>
                        <ImageBrush ImageSource="Images/CourtneyCox.jpg" Stretch="UniformToFill"></ImageBrush>
                    </Ellipse.Fill>
                </Ellipse>
                <Label Grid.Row="3" Grid.Column="1" Content="Hi! Whats's up?" Margin="18,25,0,0"  FontWeight="SemiBold" FontSize="10"></Label>
                <Label Grid.Row="3" Grid.Column="2" Content="01.20 AM" FontSize="8" Margin="0,10,0,0"></Label>
