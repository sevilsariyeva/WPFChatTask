                <Label Grid.Row="0" Grid.Column="2" Content="01.30 AM" FontSize="8" Margin="0,10,0,0"></Label>
                <Ellipse Grid.Row="0" Grid.Column="2" Margin="0,30,0,0" Fill="Red" Width="10" Height="10"></Ellipse>
                <Label Grid.Row="0" Grid.Column="2" Content="8" Foreground="White" Margin="17,29,0,0" FontWeight="Bold" FontSize="7"></Label>
                <Label Content="Lisa Kudrow" Grid.Row="1" Grid.Column="1" Margin="10,5,0,0" FontWeight="ExtraBlack"></Label>
                <Ellipse Width="45" Height="45" Grid.Row="1" Margin="-7,0,0,0">
                    <Ellipse.Fill>
                        <ImageBrush ImageSource="Images/LisaKudrow.jpg" Stretch="UniformToFill"></ImageBrush>
                    </Ellipse.Fill>

                </Ellipse>
                <Ellipse Grid.Row="1" Width="13" Height="13" Stroke="AliceBlue" StrokeThickness="2" Fill="ForestGreen" Margin="30,33,10,0">

