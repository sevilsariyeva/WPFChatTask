                <Ellipse Grid.Row="5" Width="13" Height="13" Stroke="AliceBlue" StrokeThickness="2" Fill="ForestGreen" Margin="30,33,10,0"></Ellipse>
                <Label Grid.Row="6" Grid.Column="1" Content="James Michael Tyler"  FontWeight="ExtraBlack" Margin="10,5,0,0" FontSize="10"></Label>
                <Ellipse Grid.Row="6"  Width="45" Height="45" Margin="-7,0,0,0">
                    <Ellipse.Fill>
                        <ImageBrush ImageSource="Images/JamesMichaelTyler.jpg" Stretch="UniformToFill"></ImageBrush>
                    </Ellipse.Fill>
                </Ellipse>
                <Label Grid.Row="6" Grid.Column="1" Content="Yeah!" Margin="45,25,0,0"  FontWeight="SemiBold" FontSize="10"></Label>
                <Label Grid.Row="6" Grid.Column="2" Content="8.18 PM" FontSize="8" Margin="0,10,0,0"></Label>
            </Grid>
        </Canvas>


        <Ellipse Width="38" Height="38" Grid.Row="1" Grid.Column="1" HorizontalAlignment="Left" Margin="10,0,0,0" >
            <Ellipse.Fill>
                <ImageBrush ImageSource="Images/LisaKudrow.jpg" Stretch="UniformToFill"></ImageBrush>
            </Ellipse.Fill>

        </Ellipse>
        <Label Grid.Row="1" Grid.Column="1" Content="Lisa Kudrow" FontSize="18" FontWeight="ExtraBlack" Margin="60,5,0,0"></Label>
        <Image Grid.Row="1" Grid.Column="1" Source="Images/trash.png" Width="30" Height="30" Margin="400,0,0,0"></Image>
        <Image Grid.Row="1" Grid.Column="1" Source="Images/filter.png" Width="30" Height="30" Margin="480,0,0,0"></Image>
        <Ellipse Width="42" Height="43" Grid.Row="2" Grid.Column="1" HorizontalAlignment="Left" Margin="29,16,0,326" >
            <Ellipse.Fill>
                <ImageBrush ImageSource="Images/LisaKudrow.jpg" Stretch="UniformToFill"></ImageBrush>
            </Ellipse.Fill>

        </Ellipse>
        <Label Grid.Row="2" Grid.Column="1" Margin="80,20,0,326" Background="#e6f3f9" Width="230" Height="30" HorizontalAlignment="Left">The person designing is called a designer.</Label>
        <Ellipse Width="43" Height="43" Grid.Row="2" Grid.Column="1" HorizontalAlignment="Left" Margin="29,16,0,200" >
            <Ellipse.Fill>
                <ImageBrush ImageSource="Images/LisaKudrow.jpg" Stretch="UniformToFill"></ImageBrush>
            </Ellipse.Fill>

        </Ellipse>
        <Label Grid.Row="2" Grid.Column="1" Margin="80,20,0,200" Background="#e6f3f9" Width="170" Height="30" HorizontalAlignment="Left">This is brand new information!</Label>
        <Label Width="30" Height="30" Panel.ZIndex="1" Content="VM" Grid.Column="1" Grid.Row="2" Background="Transparent" Foreground="White" FontFamily="Segoe UI" FontSize="12" Margin="470,0,0,45"></Label>
        <Ellipse Width="30" Height="30" Grid.Row="2" Grid.Column="1" Margin="470,0,0,50">
            <Ellipse.Fill>
                <LinearGradientBrush>
                    <GradientStop Color="#33d8e0" Offset="0"></GradientStop>
                    <GradientStop Color="#1795d3" Offset="1"></GradientStop>
                </LinearGradientBrush>
            </Ellipse.Fill>
        </Ellipse>
        <Label Grid.Row="2" Grid.Column="1" Margin="340,0,0,50" Background="#e6f3f9" Width="150" Height="30" HorizontalAlignment="Left">Lisa! Do you have a plan?</Label>
        <Ellipse Width="43" Height="43" Grid.Row="2" Grid.Column="1" HorizontalAlignment="Left" Margin="29,70,0,0" >
            <Ellipse.Fill>
                <ImageBrush ImageSource="Images/LisaKudrow.jpg" Stretch="UniformToFill"></ImageBrush>
            </Ellipse.Fill>

        </Ellipse>
        <Label Grid.Row="2" Grid.Column="1" Margin="80,75,0,0" Background="#e6f3f9" Width="150" Height="30" HorizontalAlignment="Left">I don’t even have a ‘pla-.</Label>
        <Ellipse Width="43" Height="43" Grid.Row="2" Grid.Column="1" HorizontalAlignment="Left" Margin="29,180,0,0" >
            <Ellipse.Fill>
                <ImageBrush ImageSource="Images/LisaKudrow.jpg" Stretch="UniformToFill"></ImageBrush>
            </Ellipse.Fill>

        </Ellipse>
        <TextBlock TextWrapping="WrapWithOverflow" Grid.Row="2" Grid.Column="1" Margin="80,195,0,0" Background="#e6f3f9" Width="350" Height="50" HorizontalAlignment="Left">Well, You Know I May Have Relatives In France Who Would Know. 
            My Grandmother Said She Got The Recipe From Her Grandmother, 
            'Nestley Toulouse'.</TextBlock>
        <Label Width="30" Height="30" Panel.ZIndex="1" Content="VM" Grid.Column="1" Grid.Row="2" Background="Transparent" Foreground="White" FontFamily="Segoe UI" FontSize="12" Margin="470,0,0,45"></Label>
    </Grid>
</Window>
