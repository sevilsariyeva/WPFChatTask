                <Ellipse Grid.Row="5"  Width="45" Height="45" Margin="-7,0,0,0">
                    <Ellipse.Fill>
                        <ImageBrush ImageSource="Images/JenniferAniston.jpg" Stretch="UniformToFill"></ImageBrush>
                    </Ellipse.Fill>
                </Ellipse>
                <Label Grid.Row="5" Grid.Column="1" Content="We were on a break!" Margin="10,25,0,0"  FontWeight="SemiBold" FontSize="10"></Label>
