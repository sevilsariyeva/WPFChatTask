                <Label Content="Typing..." Grid.Row="1" Grid.Column="1" Foreground="Green" FontWeight="SemiBold" Margin="30,22,0,0" FontSize="10"></Label>
                <Label Grid.Row="1" Grid.Column="2" Content="01.30 AM" FontSize="8" Margin="0,10,0,0"></Label>
                <Label Grid.Row="2" Grid.Column="1" Content="Matt LeBlanc"  FontWeight="ExtraBlack" Margin="10,5,0,0"></Label>
                <Ellipse Grid.Row="2"  Width="45" Height="45" Margin="-7,0,0,0">
                    <Ellipse.Fill>
                        <ImageBrush ImageSource="Images/Matt LeBlanc.jpg" Stretch="UniformToFill"></ImageBrush>
                    </Ellipse.Fill>
                </Ellipse>
                <Label Grid.Row="2" Grid.Column="1" Content="How u doin'?" Margin="18,25,0,0"  FontWeight="SemiBold" FontSize="10"></Label>
