                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="3*"/>
                    <ColumnDefinition Width="5*"/>
                    <ColumnDefinition Width="2*"/>

                </Grid.ColumnDefinitions>
                <Label Width="100" Height="30" Grid.Row="0" Grid.Column="1" Content="Matthew Perry" FontWeight="ExtraBlack" >
                </Label>
                <Ellipse Grid.Row="0" Grid.Column="0" Width="45" Height="45" Margin="10,0,0,0" HorizontalAlignment="Left">
                    <Ellipse.Fill>
                        <ImageBrush ImageSource="Images/MatthewPerry.jpg" Stretch="UniformToFill"></ImageBrush>
                    </Ellipse.Fill>
                </Ellipse>
                <Ellipse Width="13" Height="13" Stroke="AliceBlue" StrokeThickness="2" Fill="ForestGreen" Margin="30,33,10,0">

