        <Border Grid.Row="1" Grid.Column="1" BorderBrush="LightGray" BorderThickness="1"></Border>
        <Image  Grid.Row="0" Grid.Column="0" Panel.ZIndex="1" Source="Images/telegram.png" Height="30" Width="150" HorizontalAlignment="Left" Margin="25,0,0,0"/>
        <Canvas>
            <Ellipse Width="30" Height="30" Grid.Row="0" Grid.Column="1" Margin="620,8,0,0">
                <Ellipse.Fill>
                    <LinearGradientBrush>
                        <GradientStop Color="#33d8e0" Offset="0"></GradientStop>
                        <GradientStop Color="#1795d3" Offset="1"></GradientStop>
                    </LinearGradientBrush>
                </Ellipse.Fill>
            </Ellipse>
            <Label Content="VM" Grid.Column="1" Background="Transparent" Foreground="White" FontFamily="Segoe UI" FontSize="12" Margin="621,10,0,0"></Label>
